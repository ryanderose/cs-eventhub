{
  "name": "@events-hub/embed-sdk",
  "version": "0.0.0",
  "main": "dist/hub-embed.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "default": "./dist/hub-embed.umd.js",
      "require": "./dist/hub-embed.umd.js"
    },
    "./tokens.css": "./dist/tokens.css",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:deps && tsup --config tsup.config.ts --clean && tsc -p tsconfig.build.json",
    "build:deps": "tsc -p ../router-helpers/tsconfig.build.json && tsc -p ../page-schema/tsconfig.build.json && tsc -p ../telemetry/tsconfig.build.json",
    "dev": "tsup --config tsup.config.ts --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@events-hub/page-schema": "workspace:*",
    "@events-hub/router-helpers": "workspace:*",
    "@events-hub/telemetry": "workspace:*",
    "preact": "10.19.3"
  },
  "peerDependencies": {
    "preact": "^10.19.3"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "eslint": "8.57.0",
    "tsup": "^7.2.0",
    "typescript": "5.4.5",
    "vitest": "1.5.2"
  }
}
